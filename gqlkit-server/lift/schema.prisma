datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/postgres?schema=public"
}

model patient_infos {
  id                 String   @default(cuid()) @id
  date               DateTime
  pcr_total          Int
  positive_up_total  Int
  positive           Int
  positive_yesterday Int
  not_severe         Int
  severe             Int
  death_total        Int
  discharge_total    Int
}
